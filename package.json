{
  "name": "spec-runner",
  "displayName": "spec-runner",
  "description": " plugin for running specs while editing",
  "version": "0.0.1",
  "publisher": "capaj",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "title": "Open spec file",
        "category": "specrunner",
        "command": "spec-runner.openSpec"
      },
      {
        "title": "Generate a spec file",
        "category": "specrunner",
        "command": "spec-runner.generateASpec"
      },
      {
        "title": "Create '.specrunner.json' File",
        "category": "specrunner",
        "command": "spec-runner.createConfig"
      },
      {
        "title": "Show Output Channel",
        "category": "specrunner",
        "command": "spec-runner.showOutputChannel"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "pretest": "standard",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "standard": "^8.6.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "mz": "^2.6.0"
  }
}
